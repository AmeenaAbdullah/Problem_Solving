#include <iostream>
#include <string>  
using namespace std;
int main() {
	string num;
	cin >> num;    //Reading input from STDIN
	int Zs=0;
	for(int i=0;num[i]!='\0';i++){
       
		if(num[i]=='o' || num[i]=='O'){
         break;
		}
		 Zs++;
	}
	int Os=num.size()-Zs;
	Zs=Zs*2;
	
	
	if(Zs==Os){
      cout<<"Yes";
	}else{
	 cout<<"No";	
	}

}
